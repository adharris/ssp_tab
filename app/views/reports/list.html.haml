.tablewrapper
  %table
    %thead
      %tr
        %th Progranm
        %th Inventory
        %th Budget
        %th Consumption
        %th Weekly Cost
    %tbody
      - @programs.each do |program|
        %tr
          %td= program
          %td= link_to "view", url_for(:controller => :reports, :action => :inventory, :id => program.id)
          %td= link_to "view", url_for(:controller => :reports, :action => :budget, :id => program.id)
          %td= link_to "view", url_for(:controller => :reports, :action => :consumption, :id => program.id)
          %td= link_to "view", url_for(:controller => :reports, :action => :week, :id => program.id)
