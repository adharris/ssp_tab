
<% unless @active_programs.empty? %>
  <h3>Active Programs</h3>
  <%= render 'table', :programs => @active_programs %>
<% end %>

<% unless @inactive_programs.empty? %>
  <h3>Inactive Programs</h3>
  <%= render 'table', :programs => @inactive_programs %>
<% end %>

<% if can? :create, Program %>
  <%= link_to "Create a new program", new_program_path %>
<%end %>
